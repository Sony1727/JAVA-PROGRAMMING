public class Ho{
    public static void main(String a[])
    {
        Sample s1=new Sample();
        Sample s2=new Sample();
        s1.a=20;
        s2.a=35;
        s1.b=100;
        s2.b=78;
        s1.display();
        s2.display();
    }
}


   
public class Sample {
    int a;
    int b;
    void display()
    {
        System.out.println("a:"+a+"b:"+b);
    }
}
